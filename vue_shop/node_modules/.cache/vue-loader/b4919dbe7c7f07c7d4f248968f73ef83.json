i>-->\r\n          <!--<li><span>2016-02-21</span><a href=\"\">智能空气净化器翻盘：净美仕能否领衔?</a></li>-->\r\n        </ul>\r\n      </div>\r\n      <!-- 分页导航-->\r\n      <div class=\"pages\">\r\n        <a\r\n          @click=\"getData(data.pageNum - 1)\"\r\n          :class=\"{ default: data.pageNum == 1 }\"\r\n          >上一页</a\r\n        >\r\n        <!-- <a href=\"\" class=\"cur\">1</a> -->\r\n        <a\r\n          @click.prevent=\"getData(item)\"\r\n          :class=\"{ cur: data.pageNum == item }\"\r\n          v-for=\"item in data.pageCount\"\r\n          >{{ item }}</a\r\n        >\r\n        <!-- <a href=\"\">3</a>\r\n        <a href=\"\">4</a>\r\n        <a href=\"\">5</a> -->\r\n        <a\r\n          @click=\"getData(data.pageNum + 1)\"\r\n          :class=\"{ default: data.pageNum == data.pageCount }\"\r\n          >下一页</a\r\n        >\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      data: null,\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getData()\r\n  },\r\n  methods: {\r\n    getData(page = 1) {\r\n      const url = 'news_select.php?pageNum=' + page\r\n\r\n      this.axios.get(url).then(res => {\r\n        console.log(res)\r\n        this.data = res.data\r\n      })\r\n    },\r\n  },\r\n  // 过滤器\r\n  filters: {\r\n    date: value => {\r\n      // *1: 转换成数字类型(默认是字符串) 才能被识别\r\n      return new Date(value * 1).toLocaleDateString()\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.pages {\r\n  user-select: none;\r\n\r\n  .default,\r\n  .cur {\r\n    // 不接受鼠标事件\r\n    pointer-events: none;\r\n  }\r\n}\r\n</style>\r\n"]}]}